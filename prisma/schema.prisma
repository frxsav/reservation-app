generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
}
        

model User {
  id        String   @id @default(cuid())
  email     String   @unique
  password  String
  role      Role     @default(CLIENT)
  createdAt DateTime @default(now())
}

enum Role {
  ADMIN
  STAFF
  CLIENT
}
